{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "$id": "http://json-schema.org/draft-06/schema#",
    "title": "Core schema meta-schema",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "is_authorisation_required": {
                "type": "boolean"
            },
            "login_credentials": {
                "type": "object"
            },
            "test_cases": {
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "product_name": {
                            "type": "string"
                        },
                        "service_name": {
                            "type": "string"
                        },
                        "description": {
                            "type": "string"
                        },
                        "end_points": {
                            "type": "object"
                        },
                        "type": {
                            "type": "string"
                        },
                        "header": {
                            "type": "object"
                        },
                        "input": {
                            "type": "object"
                        },
                        "isStatic": {
                            "type": "boolean"
                        },
                        "responseCode": {
                            "type": "integer"
                        },
                        "expected_output": {
                            "type": "object"
                        }
                    }
                }
            }
        }
    },
    "default": {}
}